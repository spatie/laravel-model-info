{
    "name": "spatie/laravel-model-info",
    "description": "Get information about the models in your Laravel app",
    "keywords": [
        "spatie",
        "laravel",
        "laravel-model-info"
    ],
    "homepage": "https://github.com/spatie/laravel-model-info",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.13.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^2.34|^3.7",
        "pestphp/pest-plugin-laravel": "^2.3|^3.1",
        "spatie/pest-plugin-snapshots": "^2.1",
        "symfony/serializer": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ModelInfo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ModelInfo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
